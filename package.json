{
  "name": "@exodus/fetch",
  "version": "1.3.0-beta.1",
  "description": "Wrapper around global fetch / node-fetch for Electron and React",
  "repository": {
    "type": "git",
    "url": "https://github.com/ExodusMovement/fetch.git"
  },
  "bugs": {
    "url": "https://github.com/ExodusMovement/fetch/issues"
  },
  "main": "index.js",
  "exports": {
    ".": {
      "browser": "./index.browser.js",
      "react-native": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    },
    "./core": {
      "browser": "./core.browser.js",
      "react-native": "./core.native.js",
      "node": "./core.js",
      "default": "./core.js"
    },
    "./fetchival": {
      "browser": "./fetchival.browser.js",
      "react-native": "./fetchival.js",
      "node": "./fetchival.js",
      "default": "./fetchival.js"
    },
    "./wretch": {
      "browser": "./wretch.browser.js",
      "react-native": "./wretch.js",
      "node": "./wretch.js",
      "default": "./wretch.js"
    },
    "./url": {
      "browser": "./url.js",
      "react-native": "./url.js",
      "node": "./url.js",
      "default": "./url.js"
    }
  },
  "browser": {
    "./index.js": "./index.browser.js",
    "./core.js": "./core.browser.js"
  },
  "react-native": {
    "./core.js": "./core.native.js"
  },
  "files": [
    "core.browser.js",
    "core.native.js",
    "core.js",
    "fetchival.browser.js",
    "fetchival.js",
    "wretch.browser.js",
    "wretch.js",
    "url.js",
    "index.browser.js",
    "index.js"
  ],
  "scripts": {
    "lint": "prettier --list-different '**/*.js'",
    "format": "prettier --write '**/*.js'",
    "test": "tape test/*.js | tap-spec"
  },
  "author": "Exodus",
  "license": "MIT",
  "dependencies": {
    "fetchival": "^0.3.3",
    "node-fetch": "^2.6.0",
    "wretch": "^1.5.2",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "prettier": "~1.14.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.3.1"
  }
}
